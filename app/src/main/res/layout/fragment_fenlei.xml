<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/f2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   
    android:orientation="vertical" >
    
      <RelativeLayout
        android:id="@+id/fenlei_left"
        android:layout_width="50dp"
        android:background="@color/aqua"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        
          <Button  
        android:id="@+id/fenlei_left_btwaimai"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:text="外卖"
        />
           <Button  
        android:id="@+id/fenlei_left_btkuaidi"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fenlei_left_btwaimai"
        android:text="快递"
        />
     
           <Button  
        android:id="@+id/fenlei_left_btgoods"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fenlei_left_btkuaidi"
        android:text="二货"
        />
           <Button  
        android:id="@+id/fenlei_left_bt4"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fenlei_left_btgoods"
        android:text="其他"
        />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fenlei_right"
        android:layout_toRightOf="@id/fenlei_left"
        android:layout_width="match_parent"
        android:background="@color/gold"
        android:layout_height="match_parent">
         
        <WebView
         android:id="@+id/fenlei_right_webview"
        android:layout_width="320dp"
        android:layout_height="match_parent"/>
        
    </RelativeLayout>


</RelativeLayout>